create table nyc311 (
Unique_Key INT,
Created_Date VARCHAR(50),
Closed_Date VARCHAR(50),
Agency VARCHAR(50),
Agency_Name VARCHAR(50),
Complaint_Type VARCHAR(50),
Descriptor VARCHAR(50),
Location_Type VARCHAR(50),
Incident_Zip INT,
Incident_Address VARCHAR(50),
Street_Name VARCHAR(50),
Cross_Street_1 VARCHAR(50),
Cross_Street_2 VARCHAR(50),
Intersection_Street_1 VARCHAR(50),
Intersection_Street_2 VARCHAR(50),
Address_Type VARCHAR(50),
City VARCHAR(50),
Landmark VARCHAR(50),
Facility_Type VARCHAR(50),
Status VARCHAR(50),
Due_Date VARCHAR(50),
Resolution_Description VARCHAR(500),
Resolution_Action_Updated_Date VARCHAR(50),
Community_Board VARCHAR(50),
Borough VARCHAR(50),
X_Coordinate_State_Plane INT,
Y_Coordinate_State_Plane INT,
Park_Facility_Name VARCHAR(50),
Park_Borough VARCHAR(50),
School_Name VARCHAR(50),
School_Number VARCHAR(50),
School_Region VARCHAR(50),
School_Code VARCHAR(50),
School_Phone_Number VARCHAR(50),
School_Address VARCHAR(50),
School_City VARCHAR(50),
School_State VARCHAR(50),
School_Zip VARCHAR(50),
School_Not_Found VARCHAR(50),
School_or_Citywide_Complaint VARCHAR(50),
Vehicle_Type VARCHAR(50),
Taxi_Company_Borough VARCHAR(50),
Taxi_Pick_Up_Location VARCHAR(50),
Bridge_Highway_Name VARCHAR(50),
Bridge_Highway_Direction VARCHAR(50),
Road_Ramp VARCHAR(50),
Bridge_Highway_Segment VARCHAR(500),
Garage_Lot_Name VARCHAR(50),
Ferry_Direction VARCHAR(50),
Ferry_Terminal_Name VARCHAR(50),
Latitude DECIMAL(19,15),
Longitude DECIMAL(19,15), 
Location VARCHAR(100));
